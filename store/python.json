{
  "python": {
    "tool": "python@3",
    "command": "python",
    "mounts": [
      {
        "src": ".",
        "dst": "/workspace"
      }
    ],
    "env_passthrough": [
      "TERM",
      "PYTHONDONTWRITEBYTECODE",
      "PYTHONPATH"
    ]
  },
  "python3": {
    "tool": "python@3",
    "command": "python3",
    "mounts": [
      {
        "src": ".",
        "dst": "/workspace"
      }
